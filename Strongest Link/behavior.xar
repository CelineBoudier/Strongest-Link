                                                <script language="4">
                                                    <content>
                                                        <![CDATA[dPlayers = {}
sCurrentPlayer = ""
nAmount = 0
nBank = 0
nCandies = 0
dQuiz = {}

import csv, os
class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        #put initialization code here
        ALMemory.declareEvent("StrongestLink/Poke")

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        #self.onStopped() #activate the output of the box
        global dPlayers
        global sCurrentPlayer
        global nCandies
        global dQuiz
        try:
            self.sPlayersPath = self.getParameter("Players")
            self.sQuizPath = self.getParameter("Quiz")
            self.sPlayerFile = os.path.join(ALFrameManager.getBehaviorPath(self.behaviorId),self.sPlayersPath)
            self.sQuizFile = os.path.join(ALFrameManager.getBehaviorPath(self.behaviorId),self.sQuizPath)
            with open(self.sPlayerFile, 'rb') as csvfile:
                peopleReader = csv.reader(csvfile, delimiter=';', quotechar='"')
                for row in peopleReader:
                    dPlayers[row[0]] = row[1]
                    if row[2] == "Yes":
                        sCurrentPlayer = row[0]
            with open(self.sQuizFile, 'rb') as csvfile:
                quizreader = csv.reader(csvfile, delimiter=';', quotechar='"')
                for row in quizreader:
                    dQuiz[row[0]]  = row[1:4]
            nCandies = len(dQuiz.keys())
        except:
            pass
        self.onStopped()

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
</content>
